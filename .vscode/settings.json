{
    "github.copilot.enable": {
        "*": false,
        "yaml": false,
        "plaintext": false,
        "markdown": false
    },
    "l13Diff.confirmCopy": false,
    "editor.wordWrap": "on",
    "workbench.colorTheme": "Default Light+",
    "editor.minimap.enabled": false,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.telemetryLevel": "off",
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "editor.wordWrap": "on",
    "files.exclude": {
        ".idea": true,
        "__pycache__": true,
        "**/.ipynb_checkpoints/": true,
        "**/CachedExtensions/": true,
        "**/CachedExtensionVSIXs/": true,
        "**/extensions/": true,
        "**/Machine": true,
        "**/logs": true,
        "**/User": true,
        "**/machineid": true,
        "**/coder.json": true,
        "languagepacks.json": true,
        "**/.pytest_cache/": true,
        "**/.gitattributes": true,
        "**/.gitignore": true,
        "**/.dart_tool": true,
        "pubspec.lock": true,
        "analysis_options.yaml": true,
        "project/project/": true,
        "**/__pycache__": true,
    },
    "[dart]": {
        "editor.defaultFormatter": "Dart-Code.dart-code",
        "editor.formatOnSave": true
    },
    "pythonTestExplorer.testFramework": "unittest",
    "python.testing.unittestEnabled": true,
    "python.testing.cwd": "./",
    "files.watcherExclude": {
        "**/target": true
    }
}